UnlitGeneric
{
	$basetexture vgui/white
	$nofog 1
	$nodecal 1
	
	$flags_default 67125252
	$flags_mask 4 // nodraw
	$temp 0
	$zero 0
	$match 0
	$dist 0.0
	$min_dist 512.0
	
	Proxies
	{
		PlayerTeamMatch
		{
			resultVar $match
		}
		PlayerProximity
		{
			scale	  1.0
			resultVar $dist
		}
		Multiply
		{
			srcVar1   $flags_mask
			srcVar2   $match
			resultVar $temp
		}
		LessOrEqual
		{
			srcVar1		 $dist
			srcVar2		 $min_dist
			lessEqualVar $temp
			greaterVar	 $zero
			resultVar	 $temp
		}
		Subtract
		{
			srcVar1   $flags_default
			srcVar2	  $temp
			resultVar $flags
		}
	}
}